{
    "name": "firehed/deploy-command",
    "description": "Deployment command",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "console",
      "command",
      "cli",
      "deployment",
      "docker",
      "kubernetes",
      "k8s"
    ],
    "authors": [
        {
            "name": "Eric Stern",
            "email": "eric@ericstern.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "Firehed\\Console\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Firehed\\Console\\": "tests"
      }
    },
    "config": {
      "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "psr/log": "^1.0",
        "symfony/console": "^4.0",
        "symfony/process": "^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpstan",
            "@phpcs"
        ],
        "autofix": "phpcbf .",
        "phpunit": "phpunit",
        "phpstan": "phpstan analyse --no-progress -c phpstan.neon -l7 .",
        "phpcs": "phpcs ."
    }
}
